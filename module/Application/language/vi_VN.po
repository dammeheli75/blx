#: module/Application/view/application/document/index.phtml:875
msgid "cau_hoi_3_13"
msgstr "Thứ tự các xe đi như thế nào là đúng quy tắc giao thông?"

#: module/Application/view/application/document/index.phtml:885
msgid "dan_an_3_13_1"
msgstr "Xe khách, xe tải, mô tô, xe con. "

#: module/Application/view/application/document/index.phtml:886
msgid "dan_an_3_13_2"
msgstr "Xe con, xe khách, xe tải, mô tô."

#: module/Application/view/application/document/index.phtml:887
msgid "dan_an_3_13_3"
msgstr "Mô tô, xe tải, xe khách, xe con."

#: module/Application/view/application/document/index.phtml:888
msgid "dan_an_3_13_4"
msgstr "Mô tô, xe tải, xe con, xe khách."

#: module/Application/view/application/document/index.phtml:893
msgid "cau_hoi_3_14"
msgstr "Trong các trường hợp này xe nào đỗ vi phạm quy tắc giao thông?"

#: module/Application/view/application/document/index.phtml:903
msgid "dan_an_3_14_1"
msgstr "Xe tải."

#: module/Application/view/application/document/index.phtml:904
msgid "dan_an_3_14_2"
msgstr "Xe con và mô tô."

#: module/Application/view/application/document/index.phtml:905
