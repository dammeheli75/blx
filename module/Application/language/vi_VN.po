#: module/Application/view/application/document/index.phtml:857
msgid "cau_hoi_3_12"
msgstr "Biển nào báo hiệu giao nhau với đường không ưu tiên?"

#: module/Application/view/application/document/index.phtml:867
msgid "dan_an_3_12_1"
msgstr "Biển 1 và 2."

#: module/Application/view/application/document/index.phtml:868
