msgstr "Cả ba biển."

#: module/Application/view/application/document/index.phtml:822
msgid "cau_hoi_3_10"
msgstr "Biển nào báo hiệu nguy hiểm giao nhau với đường sắt?"

#: module/Application/view/application/document/index.phtml:832
msgid "dan_an_3_10_1"
msgstr "Biển 1 và 3."

#: module/Application/view/application/document/index.phtml:833
